{
  "name": "mapmaker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:fast": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:detailed": "vitest run --config vitest.config.coverage.ts --coverage",
    "test:coverage:ui": "vitest --config vitest.config.coverage.ts --ui --coverage",
    "test:coverage:watch": "vitest --config vitest.config.coverage.ts --coverage",
    "test:coverage:report": "vitest run --config vitest.config.coverage.ts --coverage && open coverage/index.html",
    "clean": "rm -rf dist node_modules .turbo coverage",
    "clean:install": "pnpm clean && pnpm install",
    "update:deps": "pnpm update --interactive --latest"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-toolbar": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "immer": "^10.1.3",
    "konva": "^10.0.2",
    "lucide-react": "^0.544.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-konva": "^19.0.10",
    "react-konva-utils": "^2.0.0",
    "use-debounce": "^10.0.6",
    "use-image": "^1.1.4",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^11.0.0",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/sprinkles": "^1.6.5",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
